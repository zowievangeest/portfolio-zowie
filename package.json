{
  "name": "portfolio-zowie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "@vuikit/icons": "0.8.1",
    "@vuikit/theme": "0.8.0",
    "global": "4.3.2",
    "serve": "7.0.0",
    "register-service-worker": "1.3.0",
    "serve": "8.1.2",
    "vue": "2.5.16",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "vuikit": "0.8.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.0-beta.15",
    "@vue/cli-service": "3.0.0-beta.16",
    "@vue/cli-plugin-unit-mocha": "3.0.0-beta.15",
    "@vue/cli-plugin-pwa": "3.0.0-beta.15",
    "@vue/cli-plugin-e2e-cypress": "3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "3.0.0-beta.16",
    "@vue/cli-service": "3.0.0-beta.15",
    "@vue/test-utils": "1.0.0-beta.18",
    "chai": "4.1.2",
    "lint-staged": "7.1.3",
    "node-sass": "4.9.0",
    "sass-loader": "7.0.3",
    "vue-template-compiler": "2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
